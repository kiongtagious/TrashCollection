@{
    ViewBag.Title = "My Profile";
}
<h2>@ViewBag.Title</h2>
<h3>@ViewBag.Message</h3>

<style>
    /* Always set the map height explicitly to define the size of the div
    * element that contains the map. */
    #map {
        height: 400px;
        width: 400px;
    }
</style>
<br />
<div class="row">
    <div id="map"></div>
    <script>
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 43.038902, lng: -87.906474 },
                zoom: 8
            });
            var labels = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ';
            var markers = locations.map(function (location, i) {
                return new google.maps.Marker({
                    position: location,
                    label: labels[i % labels.length]
                });
            });
            var markerCluster = new MarkerClusterer(map, markers,
                {imagePath: 'https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/m'});
        }
        var locations = [
            { lat: 43.034134, lng: -87.911953 },
            { lat: 43.035256, lng: -87.908521 },
            { lat: 43.035226, lng: -87.906852 },
            { lat: 43.034760, lng: -87.905779 },
            { lat: 43.034421, lng: -87.905876 }     
        ]
    </script>
    <script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js">
    </script>

      
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAmNloQ9AuAplOt-bAsVPSDyF5_Epo1W_E&callback=initMap"
            async defer></script>
</div>
&nbsp;
